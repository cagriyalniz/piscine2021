#include <unistd.h>
#include <stdio.h>//silinecek

void	ft_ultimate_div_mod(int *a, int *b)
{
	int	bos;

	*a = 10;//silincek ???
	*b = 5;// silinecek ???
	bos = *a;
	*a = *a / *b;
	*b = bos % *b;
	printf("%d\n", *a);//silinecek
	printf("%d", *b);//silinecek
}

